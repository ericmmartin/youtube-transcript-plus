{
  "name": "youtube-transcript-plus",
  "version": "1.1.2",
  "description": "Fetch transcript from a YouTube video",
  "type": "module",
  "main": "dist/youtube-transcript-plus.js",
  "module": "dist/youtube-transcript-plus.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "rollup -c",
    "format": "prettier --write 'src/**/*.ts'",
    "lint": "eslint .",
    "test": "jest",
    "test:watch": "jest --watch",
    "typecheck": "tsc --noEmit",
    "prepare": "husky"
  },
  "author": "ericmmartin",
  "keywords": [
    "youtube",
    "transcript"
  ],
  "license": "MIT",
  "lint-staged": {
    "src/**/*.ts": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.{js,json,md}": [
      "prettier --write"
    ]
  },
  "devDependencies": {
    "@eslint/js": "^9.39.2",
    "@types/jest": "^30.0.0",
    "eslint": "^9.39.2",
    "eslint-config-prettier": "^10.1.8",
    "globals": "^16.5.0",
    "https-proxy-agent": "^7.0.6",
    "husky": "^9.1.7",
    "jest": "^30.0.5",
    "lint-staged": "^16.1.5",
    "nock": "^14.0.10",
    "prettier": "^3.6.2",
    "rollup": "^4.46.4",
    "rollup-plugin-typescript": "^1.0.1",
    "rollup-plugin-typescript2": "^0.36.0",
    "ts-jest": "^29.4.1",
    "tslib": "^2.8.1",
    "typescript": "^5.9.2",
    "typescript-eslint": "^8.50.0"
  },
  "files": [
    "dist/*"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ericmmartin/youtube-transcript-plus.git"
  },
  "publishConfig": {
    "access": "public"
  },
  "homepage": "https://github.com/ericmmartin/youtube-transcript-plus",
  "engines": {
    "node": ">=18.0.0"
  }
}
