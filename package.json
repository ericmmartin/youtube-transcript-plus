{
  "name": "youtube-transcript-plus",
  "version": "1.0.2",
  "description": "Fetch transcript from a YouTube video",
  "type": "module",
  "main": "dist/youtube-transcript-plus.js",
  "module": "dist/youtube-transcript-plus.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "rollup -c",
    "format": "prettier --write 'src/**/*.ts'",
    "test": "jest",
    "test:watch": "jest --watch",
    "prepare": "husky install"
  },
  "author": "ericmmartin",
  "keywords": [
    "youtube",
    "transcript"
  ],
  "license": "MIT",
  "lint-staged": {
    "src/**/*.ts": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.{js,json,md}": [
      "prettier --write"
    ]
  },
  "devDependencies": {
    "@types/jest": "^29.5.14",
    "https-proxy-agent": "^7.0.6",
    "husky": "^9.1.7",
    "jest": "^29.7.0",
    "lint-staged": "^15.3.0",
    "prettier": "^3.4.2",
    "rollup": "^4.30.1",
    "rollup-plugin-typescript": "^1.0.1",
    "rollup-plugin-typescript2": "^0.36.0",
    "ts-jest": "^29.2.5",
    "tslib": "^2.8.1",
    "typescript": "^5.7.3"
  },
  "files": [
    "dist/*"
  ],
  "repository": "https://github.com/ericmmartin/youtube-transcript-plus.git",
  "publishConfig": {
    "access": "public"
  },
  "homepage": "https://github.com/ericmmartin/youtube-transcript-plus",
  "engines": {
    "node": ">=18.0.0"
  }
}
